                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/face49"
                    android:text="@string/face49" />

                <TextView
                    android:id="@+id/face50"
                    android:text="@string/face50" />

                <TextView
                    android:id="@+id/face51"
                    android:text="@string/face51" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/face52"
                    android:text="@string/face52" />

                <TextView
                    android:id="@+id/face53"
                    android:text="@string/face53" />

                <TextView
                    android:id="@+id/face54"
