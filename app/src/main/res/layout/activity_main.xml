            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/face16"
                    android:text="@string/face16" />

                <TextView
                    android:id="@+id/face17"
                    android:text="@string/face17" />

                <TextView
                    android:id="@+id/face18"
                    android:text="@string/face18" />

            </TableRow>

        </TableLayout>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/orangeFrame"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@+id/blueFrame"
        app:layout_constraintTop_toTopOf="@+id/blueFrame">

        <TableLayout
