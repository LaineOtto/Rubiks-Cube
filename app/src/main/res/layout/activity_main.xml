            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/face7"
                    android:text="@string/face7" />

                <TextView
                    android:id="@+id/face8"
                    android:text="@string/face8" />

                <TextView
                    android:id="@+id/face9"
                    android:text="@string/face9" />

            </TableRow>

        </TableLayout>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/blueFrame"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@+id/whiteFrame"
        app:layout_constraintTop_toBottomOf="@+id/whiteFrame">

        <TableLayout
