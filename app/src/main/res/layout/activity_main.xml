<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <FrameLayout
        android:id="@+id/whiteFrame"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="@+id/blueFrame"
        app:layout_constraintTop_toTopOf="parent">

        <TableLayout
            android:id="@+id/white"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:stretchColumns="*">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/face1"
                    android:text="@string/face1" />

                <TextView
                    android:id="@+id/face2"
                    android:text="@string/face2" />

                <TextView
                    android:id="@+id/face3"
                    android:text="@string/face3" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/face4"
                    android:text="@string/face4" />

                <TextView
                    android:id="@+id/face5"
                    android:text="@string/face5" />

                <TextView
                    android:id="@+id/face6"
                    android:text="@string/face6" />

