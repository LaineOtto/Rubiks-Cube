            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:stretchColumns="*">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/face10"
                    android:text="@string/face10" />

                <TextView
                    android:id="@+id/face11"
                    android:text="@string/face11" />

                <TextView
                    android:id="@+id/face12"
                    android:text="@string/face12" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/face13"
                    android:text="@string/face13" />

                <TextView
                    android:id="@+id/face14"
                    android:text="@string/face14" />

                <TextView
                    android:id="@+id/face15"
                    android:text="@string/face16" />

