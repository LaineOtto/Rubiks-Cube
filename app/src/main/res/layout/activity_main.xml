            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/face25"
                    android:text="@string/face25" />

                <TextView
                    android:id="@+id/face26"
                    android:text="@string/face26" />

                <TextView
