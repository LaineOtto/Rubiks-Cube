            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/face43"
                    android:text="@string/face43" />

                <TextView
                    android:id="@+id/face44"
                    android:text="@string/face44" />

                <TextView
                    android:id="@+id/face45"
                    android:text="@string/face45" />

            </TableRow>

        </TableLayout>
    </FrameLayout>

    <FrameLayout
