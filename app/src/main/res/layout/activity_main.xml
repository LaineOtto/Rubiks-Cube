            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/face31"
                    android:text="@string/face31" />

                <TextView
                    android:id="@+id/face32"
                    android:text="@string/face32" />

                <TextView
                    android:id="@+id/face33"
                    android:text="@string/face33" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/face34"
                    android:text="@string/face34" />

                <TextView
